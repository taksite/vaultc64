;---------; TRANSMISJA 1BIT; PROGRAM W STACJI;;---------         *= $0400         SEI         LDA #$7A  ;ODLACZ дипы         STA $1802         LDA #$00         STA $1800 ;CZEKAM NA WYSOKIE         LDA $1800 ;LINIE д I ц         BNE *-3PRG      LDA $1C00 ;ZMIANI         EOR #$08  ;KOLORU DIODY         STA $1C00TRANS    LDY #$08  ;8BITOWADR      LDA $C000 ;OD KAD PRZESLIJ         STA $85CONT     LDA #$02  ;ZNAK GOTOWOSCI         STA $1800         ASL A         BIT $1800 ;TEST GOTOWOSCI         BEQ *-3   ;KOMPUTRA         ASL $85         BCC JEDEN         LDA #0         STA $1800         BEQ ZEROJEDEN    LDA #$08         STA $1800ZERO     LDA #$01         BIT $1800         BEQ *-3         STA $1800         BIT $1800         BNE *-3         DEY         BNE CONT         INC ADR+1         BNE TRANS         INC ADR+2         BNE PRG         CLI         RTS