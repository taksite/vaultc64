;---------------------------------------;ÆÌÄ+ÓÃÒÏÌÌ (Ã)1993 ÃOMMODORE ËEBAB;           (Â)ÐOLONUS;---------------------------------------FB       = $FBEKRAN    = $0400         *= $1000         LDX #$01         STX $0286         DEX         STX $D020         STX $D021         JSR $E544         SEI         LDA #$1A         STA $D018         LDA #$7F         STA $DC0D         LDX #$00         STX $DC0E         STX $3FFF         INX         STX $D01A         LDA #$2E         STA $D012         LDA #$1B         STA $D011         JSR TXTSET         LDA #>IRQ         LDX #<IRQ         STA $0315         STX $0314         CLI         RTSSINUS    = $0F00DEEP     .BYTE 1COUNT1   .BYTE 0SCROLL   .BYTE 8;---------------------------------------IRQ         LDX DEEP         LDY $D012         TYA         AND #$07         EOR #$1C         BIT $FFFFIRQ1     CPY $D012         BEQ IRQ1         STA $D011         DEX         BNE IRQ+3         LDA SCROLL         STA $D016         LDX #$7B         DEX         BNE *-1         LDA #$C8         STA $D016         LDX COUNT1         LDA SINUS,X         STA DEEP         INC COUNT1         JSR DRIVER         INC $D019         JMP $EA31;---------------------------------------TXTSET         LDA #$00         STA COUNT1         LDA #7         STA SCROLLTXTSET2         LDA #>TEXT         LDX #<TEXT         STA FB+1         STX FB         RTS;---------------------------------------DRIVER         LDA SCROLL         SEC         SBC #$04         STA SCROLL         BMI DRIVER2         RTSDRIVER2         LDA #$07         STA SCROLL         LDX #$00DRIVER3         LDA EKRAN+1,X         STA EKRAN,X         INX         CPX #$27         BCC DRIVER3         LDY #0DRIVER4         LDA (FB),Y         BNE DRIVER5         JSR TXTSET2         JMP DRIVER4DRIVER5        ;AND #$3F         STA $0427         INC FB         BNE *+4         INC FB+1         RTSTEXT         .TEXT " ALA MA KOTA  MILICJA"         .TEXT " MA PALE !!!"         .BYTE 0